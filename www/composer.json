{
    "name": "typearea/dembelo",
    "license": "AGPL",
    "type": "project",
    "description": "Dembelo ist Software fÃ¼r das granulierte und vernetzte Lesen neuer Unterhaltungsliteratur, der ein netztypischer, verzweigter Aufbau zugrundeliegt.",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
      "php": ">=7.1.0",
      "symfony/symfony": "3.4.*",
      "doctrine/orm": "^2.5",
      "doctrine/dbal": "2.6.*",
      "doctrine/doctrine-bundle": "^1.8",
      "symfony/assetic-bundle": "2.8.*",
      "symfony/swiftmailer-bundle": "^3.1",
      "symfony/monolog-bundle": "^3.1",
      "sensio/distribution-bundle": "^5.0",
      "sensio/framework-extra-bundle": "^5.1",
      "sensio/generator-bundle": "^3.1",
      "incenteev/composer-parameter-handler": "~2.1",
      "twig/twig": "^2.4",
      "typearea/hyphenator": "dev-master",
      "doctrine/mongodb-odm": "1.2.*",
      "doctrine/mongodb-odm-bundle": "3.4.*",
      "etdsolutions/domready": "dev-master",
      "etdsolutions/requirejs": "^2.3",
      "typearea/webix": "^4.1",
      "alcaeus/mongo-php-adapter": "1.0.6",
      "sllh/composer-versions-check": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.*",
        "symfony/phpunit-bridge": "^4.0",
        "escapestudios/symfony2-coding-standard": "~3.0",
        "apoutchika/loremipsum-bundle": "dev-master",
        "symfony/debug": "4.0.*"
    },
    "scripts": {
        "post-root-package-install": [
          "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts",
            "DembeloMain\\Composer\\ScriptHandler::buildSymLinks"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 600
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
